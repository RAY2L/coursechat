<div class="container card my-3">
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p>
        <%- course_section["Instructors"].join(", ") %> - <%=
        course_section["Academic Year"] %> <%= course_section["Academic Term"]
        %>
      </p>
    </blockquote>
    <% if (summaries[course_section["Id"]]) { %>
    <span class="badge text-bg-success">Has Summary</span>
    <% } %>
    <a
      href="/<%= subject %>/<%= courseId %>/eval/<%= course_section['Id'] %>"
      class="stretched-link"
    ></a>
  </div>
</div>
